<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="sapar_mfi.finder" class="Sapar\MfiBundle\Finder">
            <argument type="service" id="debug.event_dispatcher"/>
        </service>
        <service id="sapar_mfi.finder_beforefiltercollection_listener" class="Sapar\MfiBundle\Listener\MfiListener">
            <argument type="service" id="sapar_id3.id3.manager"/>
            <tag name="kernel.event_listener" event="mfi.finder.before.collection.filter" />
        </service>
    </services>

</container>
